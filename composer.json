{
  "name": "juancarlo99/pdf-pades-signature-extractor",
  "description": "PHP library to extract PAdES digital signature data from signed PDF files",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Juan Carlos Nunes",
      "homepage": "https://github.com/juancarlo99"
    }
  ],
  "autoload": {
    "psr-4": {
      "Juancarlo99\\PdfPadesSignatureExtractor\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Juancarlo99\\PdfPadesSignatureExtractor\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "ext-openssl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "squizlabs/php_codesniffer": "^3.7",
    "phpstan/phpstan": "^1.12"
  },
  "scripts": {
    "test": "phpunit",
    "lint": "phpcs --standard=phpcs.xml src tests",
    "stan": "phpstan analyse -c phpstan.neon"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
